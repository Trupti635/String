# String
package com.stringex;

public class StringDEmo2 {

	public static void main(String[] args) {
		String s1="Ravi";
		String s2="Shyam";
		String s3="Ravi";
		 System.out.println(s1==s2);
		 System.out.println(s1==s3);

		System.out.println(s1.length());
		System.out.println(s2.length());
		 
	}

}
